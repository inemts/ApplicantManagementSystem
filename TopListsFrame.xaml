<Page x:Class="Student.TopListsFrame"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Student"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TopListsFrame">
    <Grid>
        <Grid>
            <Button Content="Назад" BorderBrush="{x:Null}" Background="#FF638DFF" Foreground="White" Padding="20,5,20,5" FontSize="16" Click="BackBtn" HorizontalAlignment="Left" Width="100" VerticalAlignment="Top"/>
            <TextBlock  FontFamily="Cascadia code" FontSize="18" HorizontalAlignment="Center" >Рейтинг абитуриентов подавших заявку на специальность</TextBlock>
            <TextBlock FontFamily="Cascadia code" FontSize="18" HorizontalAlignment="Center" Margin="0,20,0,0" Name="NameSpecial">Машинное дело</TextBlock>
            <StackPanel Margin="0,80,0,0">
                <ScrollViewer MaxHeight="350">
                    <DataGrid></DataGrid>
                </ScrollViewer>
            </StackPanel>
        </Grid>
    </Grid>
</Page>
